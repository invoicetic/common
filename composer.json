{
  "name": "invoicetic/common",
  "type": "library",
  "license": "MIT",
  "description": "invoice multigateway base objects",
  "keywords": [],
  "homepage": "https://github.com/invoicetic/common",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Invoicetic\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Invoicetic\\Common\\Tests\\": "tests/src",
      "Invoicetic\\Common\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "php-http/client-implementation": "^1",
    "php-http/message": "^1.5",
    "php-http/message-factory": "^1.1",
    "php-http/discovery": "^1.14",
    "symfony/http-foundation": "^2.1|^3|^4|^5|^6"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "php-http/mock-client": "^1",
    "php-http/guzzle7-adapter": "^1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true,
      "php-http/discovery": true
    }
  }
}
